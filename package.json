    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "helmet": "^3.18.0",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/hapi__boom": "^7.4.0",
    "@types/helmet": "0.0.43",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.8",
    "nodemon": "^1.19.1",
