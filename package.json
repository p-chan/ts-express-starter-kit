  "description": "The fuckin' awesome beautiful starter kit",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node .",
    "build": "tsc",
    "dev": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts",
